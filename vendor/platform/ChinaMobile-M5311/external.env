# Compiler
ROOTPATH = $(CURDIR)/..
TOOLPATH = $(ROOTPATH)/GNU-Tools-ARM-Embedded/4.9-2015q3/bin
CC := $(TOOLPATH)/arm-none-eabi-gcc
#CXX := $(TOOLPATH/arm-none-eabi-g++
AR := $(TOOLPATH)/arm-none-eabi-ar

# Commands
CYGWIN_BASE := C:\cygwin64
BOAT_RM := $(CYGWIN_BASE)\bin\rm -rf
BOAT_MKDIR := $(CYGWIN_BASE)\bin\mkdir
BOAT_FIND := $(CYGWIN_BASE)\bin\find


#LD_LIBRARY_PATH := $(LD_LIBRARY_PATH):$(ROOTPATH)/lib/LVobj.a

# External include path and libraries

EXTERNAL_INC := -I$(ROOTPATH)/GNU-Tools-ARM-Embedded/4.9-2015q3/lib/gcc/arm-none-eabi/4.9.3/include  -I$(ROOTPATH)/inc/os -I$(ROOTPATH)/inc/apb -I$(ROOTPATH)/inc/lwip -I$(ROOTPATH)/src -I$(ROOTPATH). -I$(ROOTPATH)/inc -I$(ROOTPATH)/BoAT-X-Framework/vendor/platform/ChinaMobile-M5311/src/cJSON_extension

EXTERNAL_CFLAGS := -mlittle-endian -mthumb -mcpu=cortex-m4 -fsingle-precision-constant -Wdouble-promotion -mfpu=fpv4-sp-d16 -mfloat-abi=hard -ffunction-sections -fdata-sections -fno-builtin -gdwarf-2 -Os -fno-strict-aliasing -fno-common  -std=gnu99 -w
#EXTERNAL_CFLAGS :=

EXTERNAL_LFLAGS :=



export CC
export AR
export BOAT_RM
export BOAT_MKDIR
export BOAT_FIND
export LD_LIBRARY_PATH
export EXTERNAL_INC
export EXTERNAL_LIBS
export EXTERNAL_CFLAGS
export EXTERNAL_LFLAGS
